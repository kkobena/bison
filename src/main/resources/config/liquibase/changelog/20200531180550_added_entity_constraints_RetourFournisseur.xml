<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity RetourFournisseur.
    -->
    <changeSet id="20200531180550-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="magasin_id"
                                 baseTableName="retour_fournisseur"
                                 constraintName="fk_retour_fournisseur_magasin_id"
                                 referencedColumnNames="id"
                                 referencedTableName="magasin"/>

        <addForeignKeyConstraint baseColumnNames="created_by_id"
                                 baseTableName="retour_fournisseur"
                                 constraintName="fk_retour_fournisseur_created_by_id"
                                 referencedColumnNames="id"
                                 referencedTableName="utilisateur"/>

        <addForeignKeyConstraint baseColumnNames="motif_id"
                                 baseTableName="retour_fournisseur"
                                 constraintName="fk_retour_fournisseur_motif_id"
                                 referencedColumnNames="id"
                                 referencedTableName="motif"/>

    </changeSet>
</databaseChangeLog>
