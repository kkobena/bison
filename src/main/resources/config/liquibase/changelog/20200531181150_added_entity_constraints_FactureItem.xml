<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity FactureItem.
    -->
    <changeSet id="20200531181150-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="facture_id"
                                 baseTableName="facture_item"
                                 constraintName="fk_facture_item_facture_id"
                                 referencedColumnNames="id"
                                 referencedTableName="facture"/>

        <addForeignKeyConstraint baseColumnNames="client_id"
                                 baseTableName="facture_item"
                                 constraintName="fk_facture_item_client_id"
                                 referencedColumnNames="id"
                                 referencedTableName="client"/>

        <addForeignKeyConstraint baseColumnNames="ayant_droit_id"
                                 baseTableName="facture_item"
                                 constraintName="fk_facture_item_ayant_droit_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ayant_droit"/>

    </changeSet>
</databaseChangeLog>
