<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Paiement.
    -->
    <changeSet id="20200531181850-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="magasin_id"
                                 baseTableName="paiement"
                                 constraintName="fk_paiement_magasin_id"
                                 referencedColumnNames="id"
                                 referencedTableName="magasin"/>

        <addForeignKeyConstraint baseColumnNames="created_by_id"
                                 baseTableName="paiement"
                                 constraintName="fk_paiement_created_by_id"
                                 referencedColumnNames="id"
                                 referencedTableName="utilisateur"/>

        <addForeignKeyConstraint baseColumnNames="banque_id"
                                 baseTableName="paiement"
                                 constraintName="fk_paiement_banque_id"
                                 referencedColumnNames="id"
                                 referencedTableName="banque"/>

    </changeSet>
</databaseChangeLog>
