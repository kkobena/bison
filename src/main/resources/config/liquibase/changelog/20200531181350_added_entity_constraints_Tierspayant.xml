<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Tierspayant.
    -->
    <changeSet id="20200531181350-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="groupetp_id"
                                 baseTableName="tierspayant"
                                 constraintName="fk_tierspayant_groupetp_id"
                                 referencedColumnNames="id"
                                 referencedTableName="groupe_tierspayant"/>

        <addForeignKeyConstraint baseColumnNames="risque_id"
                                 baseTableName="tierspayant"
                                 constraintName="fk_tierspayant_risque_id"
                                 referencedColumnNames="id"
                                 referencedTableName="risque"/>

        <addForeignKeyConstraint baseColumnNames="model_facture_id"
                                 baseTableName="tierspayant"
                                 constraintName="fk_tierspayant_model_facture_id"
                                 referencedColumnNames="id"
                                 referencedTableName="model_facture"/>

    </changeSet>
</databaseChangeLog>
