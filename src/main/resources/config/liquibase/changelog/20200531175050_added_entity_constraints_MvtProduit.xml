<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity MvtProduit.
    -->
    <changeSet id="20200531175050-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="magasin_id"
                                 baseTableName="mvt_produit"
                                 constraintName="fk_mvt_produit_magasin_id"
                                 referencedColumnNames="id"
                                 referencedTableName="magasin"/>

        <addForeignKeyConstraint baseColumnNames="created_by_id"
                                 baseTableName="mvt_produit"
                                 constraintName="fk_mvt_produit_created_by_id"
                                 referencedColumnNames="id"
                                 referencedTableName="utilisateur"/>

        <addForeignKeyConstraint baseColumnNames="produit_id"
                                 baseTableName="mvt_produit"
                                 constraintName="fk_mvt_produit_produit_id"
                                 referencedColumnNames="id"
                                 referencedTableName="stock_produit"/>

    </changeSet>
</databaseChangeLog>
