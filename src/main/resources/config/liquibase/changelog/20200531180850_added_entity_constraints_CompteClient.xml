<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity CompteClient.
    -->
    <changeSet id="20200531180850-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="client_id"
                                 baseTableName="compte_client"
                                 constraintName="fk_compte_client_client_id"
                                 referencedColumnNames="id"
                                 referencedTableName="client"/>

        <addForeignKeyConstraint baseColumnNames="tierspayant_id"
                                 baseTableName="compte_client"
                                 constraintName="fk_compte_client_tierspayant_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tierspayant"/>

    </changeSet>
</databaseChangeLog>
