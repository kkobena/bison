<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity LignesVente.
    -->
    <changeSet id="20200531181650-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="vente_id"
                                 baseTableName="lignes_vente"
                                 constraintName="fk_lignes_vente_vente_id"
